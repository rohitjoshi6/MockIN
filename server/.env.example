# Copy this file to .env and fill in your actual API keys
# DO NOT COMMIT THE ACTUAL .env FILE TO GIT

# Judge0 API Key from RapidAPI
# Get it from: https://rapidapi.com/judge0-official/api/judge0-ce
JUDGE0_API_KEY=your_judge0_api_key_here

# OpenAI API Key
# Get it from: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Set to true to use mock feedback instead of real OpenAI API
USE_MOCK_FEEDBACK=false
